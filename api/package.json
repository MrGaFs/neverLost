{
  "name": "never-lost-api",
  "version": "0.1.0",
  "description": "never lost graduation project api",
  "main": "server.ts",
  "author": "MrGAFs",
  "license": "MIT",
  "scripts": {
    "start": "node src/server.ts",
    "clean": "rm -rf dist",
    "build": "yarn clean && npx tsc",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "jasmine": "jasmine",
    "test": "ENV=test db-migrate --env test up ; ENV=test yarn build ; ENV=test yarn jasmine ; db-migrate --env test down ",
    "migrate": "db-migrate up",
    "rollback": "db-migrate down",
    "prettier": "prettier --write 'src/**/*.ts'",
    "lint": "eslint --fix 'src/**/*.ts'"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "pg-pool": "^3.5.1",
    "yarn": "^1.22.18"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/pg": "^8.6.5",
    "@types/pg-pool": "^2.0.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^4.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "pg-client": "^0.0.5",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.3"
  }
}
